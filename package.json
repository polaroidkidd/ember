{
	"name": "@dle.dev/ember",
	"version": "1.1.1",
	"author": {
		"name": "Daniel Einars",
		"email": "github@dle.dev"
	},
	"license": "MIT",
	"description": "A headless, accessible, and highly customizable accordion component for Svelte.",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:polaroidkidd/ember.git"
	},
	"bugs": {
		"url": "https://github.com/polaroidkidd/ember/issues"
	},
	"private": false,
	"homepage": "https://ember.dle.dev",
	"scripts": {
		"dev": "vite dev",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"gen:exports": "svelte-lib-helpers exports",
		"gen:docspropvalue": "svelte-lib-helpers docspropvalue polaroidkidd/ember",
		"gen:component-data-prop-value": "svelte-lib-helpers component-data-prop-value polaroidkidd/ember",
		"copy:packagejson": "svelte-lib-helpers package",
		"lib-helpers": "pnpm gen:component-data-prop-value && pnpm gen:docspropvalue && pnpm format && pnpm prepack && pnpm gen:exports && pnpm copy:packagejson && pnpm format",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"preview:cf": "pnpm build && wrangler pages dev ./.svelte-kit/cloudflare",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint \"./src/**/*.{js,ts,svelte}\"",
		"lint:fix": "eslint \"./src/**/*.{js,ts,svelte}\" --fix",
		"test": "vitest --run",
		"test:ui": "vitest --watch",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"publish-packages": "changeset publish"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"src/lib",
		"!src/lib/**/*.test.*",
		"!src/lib/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/package": "^2.5.6",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"@vitest/browser": "^3.2.3",
		"@vitest/ui": "3.2.4",
		"clsx": "^2.1.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^3.13.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"eslint-plugin-yml": "^1.18.0",
		"globals": "^16.5.0",
		"highlight.js": "^11.11.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"playwright": "1.57.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"publint": "^0.3.15",
		"sass-embedded": "^1.93.3",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"svelte-highlight": "^7.9.0",
		"svelte-lib-helpers": "^0.4.31",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"uuid": "^13.0.0",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0",
		"wrangler": "^4.51.0",
		"yaml-eslint-parser": "^1.3.1"
	},
	"keywords": [
		"svelte",
		"headless"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild",
			"sharp",
			"workerd"
		]
	},
	"packageManager": "pnpm@10.17.1",
	"engines": {
		"node": ">=24.7.0",
		"pnpm": ">=10.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"import": "./dist/types.js",
			"default": "./dist/types.js"
		},
		"./tree.svelte": {
			"types": "./dist/tree/tree.svelte.d.ts",
			"svelte": "./dist/tree/tree.svelte"
		},
		"./package.json": "./package.json"
	}
}
