{
	"name": "@dle.dev/ember",
	"version": "0.0.4",
	"author": {
		"name": "Daniel Einars",
		"email": "github@dle.dev"
	},
	"license": "MIT",
	"description": "A headless, accessible, and highly customizable accordion component for Svelte.",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:polaroidkidd/ember.git"
	},
	"bugs": {
		"url": "https://github.com/polaroidkidd/ember/issues"
	},
	"private": false,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"preview:cf": "pnpm build && wrangler pages dev ./.svelte-kit/cloudflare",
		"prepare": "husky",
		"prepack": "svelte-kit sync && svelte-package -i ./src/lib/ember && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint \"./src/**/*.{js,ts,svelte}\"",
		"lint:fix": "eslint \"./src/**/*.{js,ts,svelte}\" --fix",
		"test": "vitest --run",
		"test:ui": "vitest --watch",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"publish-packages": "changeset publish",
		"ci:publish": "pnpm publish -r"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22",
		"@types/uuid": "^11.0.0",
		"@vitest/browser": "^3.2.3",
		"@vitest/ui": "3.2.4",
		"clsx": "^2.1.1",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^3.0.0",
		"eslint-plugin-unused-imports": "^4.2.0",
		"eslint-plugin-yml": "^1.18.0",
		"globals": "^16.0.0",
		"highlight.js": "^11.11.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.3",
		"playwright": "1.55.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"sass-embedded": "^1.93.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-highlight": "^7.8.4",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"uuid": "^13.0.0",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0",
		"wrangler": "^4.40.3",
		"yaml-eslint-parser": "^1.3.0"
	},
	"keywords": [
		"svelte",
		"headless"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild",
			"sharp",
			"workerd"
		]
	},
	"packageManager": "pnpm@10.17.1",
	"engines": {
		"node": ">=24.7.0",
		"pnpm": ">=10.0.0"
	}
}
